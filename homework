// Задача 1
function func1(a,b){
  console.log('Задача 1');
  let n;
  if (a > b){
    n = a - b;
    console.log(`Число ${a} > ${b} на ${n}`);
  }
  else if (b > a){
    n = b - a;
    console.log(`Число ${b} > ${a} на ${n}`);
  }
  else{
    console.log("Число a и b равны");
  }
}  

func1(634,132);

