// Задача 7

let arr7 = [1,2,3,4];
function func7(arr7){
  console.log('Задача 7');
  let sum = 0;
  for(let i = 0; i < arr7.length; i++){
    sum+=arr7[i];
  }
  console.log("Сумма = " + sum);
}

func7(arr7);

